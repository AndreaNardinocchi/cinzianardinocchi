  const wind_speed_10m_max = document.getElementById("wind_speed_10m_max");
  const apparent_temperature_min = document.getElementById("apparent_temperature_min");
  const time = document.getElementById("time");
  const weatherCode = document.getElementById("weatherCode");
  
  // ICONS
  let iconDailyToday=[];
  
  iconDailyToday.push('/images/storm.png');
  iconDailyToday.push('/images/cloudy.png');
  iconDailyToday.push('/images/sun.png');
  
  if (currentCityData.daily.weather_code[0] === 0) {
   iconDailyToday = '/images/sun.png';
  } else if ((currentCityData.daily.weather_code[0] >= 1) && (currentCityData.daily.weather_code[0] <=3)) {
   iconDailyToday = '/images/cloudy.png';
  } else {
   iconDailyToday = '/images/storm.png';
  }
  
  // Berlin Card
  
  const dashboardCardTitleBerlin = document.querySelector('#cityfocus-1-berlin-heading');
  dashboardCardTitleBerlin.innerHTML = 'Berlin';
  
  const dashboardBerlinCardImage = document.querySelector('#cityfocus-1-berlin-card-image');
  dashboardBerlinCardImage.src = iconDailyToday;
  
  const dashboardBerlinCardTemp = document.querySelector('#cityfocus-1-berlin-card-temp');
  dashboardBerlinCardTemp.innerHTML = currentCityData.daily.temperature_2m_max[0] + ' 째C';
  
  const dashboardBerlinCardWind = document.querySelector('#cityfocus-1-berlin-card-wind');
